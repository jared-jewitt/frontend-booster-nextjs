{
  "name": "frontend-booster-nextjs",
  "version": "1.0.0",
  "description": "NextJS boilerplate",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "prod": "cross-env NODE_ENV=production next start -p ${PORT:-3002}",
    "dev": "cross-env NODE_ENV=development next dev -p ${PORT:-3000}",
    "test": "cross-env NODE_ENV=test npm run test:unit && npm run test:integration:headless",
    "test:unit": "cross-env NODE_ENV=test TS_NODE_COMPILER_OPTIONS='{\"module\": \"CommonJS\" }' mocha",
    "test:integration": "cross-env NODE_ENV=test PORT=${PORT:-3001} start-server-and-test dev http://localhost:${PORT:-3001} 'cypress open'",
    "test:integration:headless": "cross-env NODE_ENV=test PORT=${PORT:-3001} start-server-and-test dev http://localhost:${PORT:-3001} 'cypress run'"
  },
  "dependencies": {
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "cross-env": "^7.0.3",
    "next": "^11.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.42.1",
    "sharp": "^0.29.1",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/faker": "^5.5.8",
    "@types/mocha": "^9.0.0",
    "@types/sinon": "^10.0.3",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "chai": "^4.3.4",
    "cypress": "^8.4.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.5.3",
    "husky": "^7.0.2",
    "jsdom": "17.0.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "^11.1.2",
    "mocha": "^9.1.1",
    "prettier": "^2.4.1",
    "sinon": "^11.1.2",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-less": "^1.0.1",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0"
  }
}
